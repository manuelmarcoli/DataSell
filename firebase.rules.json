{
  "rules": {
    "transactions": {
      ".read": "auth != null",
      ".write": "auth != null",
      ".indexOn": ["userId"]
    },
    "payments": {
      ".read": "auth != null",
      ".write": "auth != null",
      ".indexOn": ["userId"]
    }
    ,
    "notifications": {
      ".read": "auth != null",
      ".write": "auth != null",
      ".indexOn": ["createdAt"]
    }
  }
}
